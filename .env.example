# AI Provider Selection
# Options: openai, gemini, ollama
AI_PROVIDER=openai

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_MODEL=gpt-4o-mini

# Google Gemini Configuration
GOOGLE_API_KEY=
GEMINI_MODEL=gemini-1.5-pro

# Local AI Configuration (Ollama, LM Studio, LocalAI, etc.)
# Base URL should be without /v1 suffix (it's added automatically)
OLLAMA_BASE_URL=http://localhost:1234
OLLAMA_MODEL=llama-3.2-1b-instruct

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_client_id
GOOGLE_CLIENT_SECRET=your_client_secret
OAUTH_SERVER_URL=http://localhost:8502
STREAMLIT_URL=http://localhost:8501
FLASK_SECRET_KEY=your_random_secret_key_here
OAUTH_PORT=8502

# GitHub API Configuration (Optional - for higher rate limits and private repos)
GITHUB_TOKEN=
